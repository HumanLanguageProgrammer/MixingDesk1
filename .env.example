# Supabase Configuration
# Copy this file to .env and fill in your actual values
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=your-anon-key-here

# Phase B: Anthropic API (server-side only, no VITE_ prefix)
# This key is ONLY used in Vercel Functions, never exposed to browser
ANTHROPIC_API_KEY=your-anthropic-api-key-here

# Phase B: Agent Configuration
VITE_AGENT_NAME=testing-agent

# Phase C: Hume AI Voice Configuration (server-side only, no VITE_ prefix)
# Get these from your Hume AI dashboard: https://platform.hume.ai
HUME_API_KEY=your-hume-api-key-here
HUME_VOICE_ID=your-featured-voice-id-here

# Phase C: Voice Feature Flag (client-side)
# Set to "true" to enable voice features
VITE_VOICE_ENABLED=true
